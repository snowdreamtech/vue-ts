{
  "name": "vue-ts",
  "private": true,
  "version": "0.0.0",
  "keywords": [
    "vue",
    "esbuild"
  ],
  "type": "module",
  "author": "SnowdreamTech Inc",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/snowdreamtech/vs-ts.git"
  },
  "bugs": {
    "url": "https://github.com/snowdreamtech/vs-ts/issues"
  },
  "homepage": "https://github.com/snowdreamtech",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "stage": "vue-tsc && vite build --mode staging",
    "testing": "vue-tsc && vite build --mode testing",
    "preview": "vite preview",
    "lint": "pnpm lint:eslint && pnpm lint:stylelint",
    "lint:eslint": "eslint --fix --ext .js,.jsx,.vue,.ts,.tsx .",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss,sass,styl}\"",
    "lint:commitlint": "commitlint --edit",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "3.5.13"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@rushstack/eslint-patch": "1.5.1",
    "@types/node": "22.13.4",
    "@vitejs/plugin-vue": "5.2.1",
    "@vue/eslint-config-typescript": "12.0.0",
    "eslint": "8.51.0",
    "eslint-plugin-vue": "9.17.0",
    "husky": "9.1.7",
    "stylelint": "16.14.1",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-recommended": "15.0.0",
    "stylelint-config-recommended-less": "3.0.1",
    "stylelint-config-recommended-scss": "14.1.0",
    "stylelint-config-recommended-vue": "1.6.0",
    "stylelint-config-standard": "37.0.0",
    "stylelint-config-standard-scss": "14.0.0",
    "stylelint-less": "3.0.1",
    "stylelint-order": "6.0.4",
    "typescript": "5.7.3",
    "vite": "6.1.0",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-stylelint": "6.0.0",
    "vue-tsc": "2.2.0"
  }
}
